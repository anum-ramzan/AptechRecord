<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Aptech Record</title>
    <link rel="icon" type="image/png" href="~/images/icons/favicon.ico">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/responsive.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/buttons.dataTables.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/_all-skins.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-select.min.css" rel="stylesheet" />
    <link href="~/Content/admin-style.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.responsive.js"></script>
    <script src="~/Scripts/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/dataTables.rowsGroup.js"></script>
    <script src="~/Scripts/bootstrap-select.min.js"></script>
    <script src="~/Scripts/buttons.flash.min.js"></script>
    <script src="~/Scripts/jszip.min.js"></script>
    <script src="~/Scripts/pdfmake.min.js"></script>
    <script src="~/Scripts/vfs_fonts.js"></script>
    <script src="~/Scripts/buttons.html5.min.js"></script>
    <script src="~/Scripts/buttons.print.min.js"></script>
    <script src="~/Scripts/adminlte.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script src="http://code.highcharts.com/modules/exporting.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/admin-js.js"></script>


</head>
@*<body class="hold-transition skin-yellow sidebar-mini" oncontextmenu="return false" onkeydown="return false;" onmousedown="return false;">*@
<body class="hold-transition skin-yellow sidebar-mini">
    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            @if (Session["UserRole"].ToString() == "1")
            {
                <a href=@Url.Action("Dashboard", "Admin") class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>AEC</b></span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><b>Aptech</b>Record</span>
                </a>
            }
            else if (Session["UserRole"].ToString() == "2")
            {
                <a href=@Url.Action("Index", "Management") class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>AEC</b></span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><b>Aptech</b>Record</span>
                </a>
            }
            else if (Session["UserRole"].ToString() == "3")
            {
                <a href=@Url.Action("Index", "Admission") class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>AEC</b></span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><b>Aptech</b>Record</span>
                </a>
            }
            else if (Session["UserRole"].ToString() == "4")
            {
                <a href=@Url.Action("Index", "Coordinator") class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>AEC</b></span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><b>Aptech</b>Record</span>
                </a>
            }



            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href=@Url.Action("Logout", "Accounts") class="dropdown-toggle">
                                <i class="fa fa-sign-out"></i>
                                <span>Log out</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel">
                    <h4>@Session["Name"].ToString()</h4>
                </div>
                <!-- Sidebar Menu -->
                @{
                    if (Session["UserRole"].ToString() == "1")
                    {
                        #region Administration Side MENUs

                        <ul class="sidebar-menu" data-widget="tree">
                            <li><a href=@Url.Action("Dashboard", "Admin")><i class="fa fa-dashboard"></i> <span>Dashboard</span></a></li>
                            <li><a href=@Url.Action("Index", "Users")><i class="fa fa-dashboard"></i> <span>Users</span></a></li>
                            <li><a href=@Url.Action("TodaysReport", "Attendances")><i class="fa fa-list"></i> <span>Today Attendance</span></a></li>
                            <li><a href=@Url.Action("TwoDaysReport", "Attendances")><i class="fa fa-list"></i> <span>Last Two Days (MWF & TTS) <br /> Attendance</span></a></li>
                            <li><a href=@Url.Action("UnMarkBatches", "Attendances")><i class="fa fa-list"></i> <span>Unmark Batches Attendance</span></a></li>
                            <li><a href=@Url.Action("FacultyWiseAttendance", "Attendances")><i class="fa fa-list"></i> <span>Faculty Wise Attendance</span></a></li>
                            <li><a href=@Url.Action("ConsolidatedAttendance", "Attendances")><i class="fa fa-list"></i> <span>Consolidated Attendance</span></a></li>
                            <li><a href=@Url.Action("DateWiseAttendance", "Attendances")><i class="fa fa-calendar"></i> <span>Date-Wise Attendance Data</span></a></li>
                            <li><a href=@Url.Action("StudentWiseAttendance", "Attendances")><i class="fa fa-calendar"></i> <span>Student-Wise Attendance Data</span></a></li>
                            <li><a href=@Url.Action("SessionWiseAttendance", "Attendances")><i class="fa fa-calendar"></i> <span>Session Wise Attendance</span></a></li>
                            <li><a href=@Url.Action("TimingWiseAttendance", "Attendances")><i class="fa fa-calendar"></i> <span>Timing Wise Attendance</span></a></li>
                            <li><a href=@Url.Action("ZeroOrNoAttendance", "Attendances")><i class="fa fa-calendar"></i> <span>Student with Zero/No Attendance</span></a></li>
                            <li><a href=@Url.Action("LowAttendance", "Attendances")><i class="fa fa-calendar"></i> <span>Student with Low Attendance</span></a></li>
                            <li><a href=@Url.Action("Customized", "Attendances")><i class="fa fa-calendar"></i> <span>Customized Attendance Data</span></a></li>
                            <li><a href=@Url.Action("VoucherReport", "Vouchers")><i class="fa fa-list"></i> <span>Voucher Report</span></a></li>
                            <li><a href=@Url.Action("Index", "Students")><i class="fa fa-dashboard"></i> <span>Students</span></a></li>
                            <li><a href=@Url.Action("Index", "Batches")><i class="fa fa-dashboard"></i> <span>Batch</span></a></li>
                            <li><a href=@Url.Action("Index", "SlotAssigns")><i class="fa fa-dashboard"></i> <span>Recovery Slot Setting</span></a></li>
                            <li><a href=@Url.Action("Index", "Course")><i class="fa fa-dashboard"></i> <span>Courses</span></a></li>
                            <li><a href=@Url.Action("Index", "CourseDetails")><i class="fa fa-dashboard"></i> <span>Course Details</span></a></li>
                            <li><a href=@Url.Action("Index", "Semesters")><i class="fa fa-dashboard"></i> <span>Semester</span></a></li>
                            <li><a href=@Url.Action("Index", "Books")><i class="fa fa-dashboard"></i> <span>Book</span></a></li>
                            <li><a href=@Url.Action("Index", "TimeSlots")><i class="fa fa-dashboard"></i> <span>Time Slot</span></a></li>
                            <li><a href=@Url.Action("Index", "Employees")><i class="fa fa-dashboard"></i> <span>Employees</span></a></li>
                            <li><a href=@Url.Action("PasswordChange", "Admin")><i class="fa fa-dashboard"></i> <span>Change Password</span></a></li>
                        </ul>
                        #endregion
                    }
                    else if (Session["UserRole"].ToString() == "2")
                    {
                        #region Management Side MENUs
                        <ul class="sidebar-menu" data-widget="tree">
                            <li><a href=@Url.Action("Index", "Management")><i class="fa fa-dashboard"></i> <span>Dashboard</span></a></li>
                            <li><a href=@Url.Action("TodaysReport", "Attendances")><i class="fa fa-user"></i> <span>Daily Report</span></a></li>
                            <li><a href=@Url.Action("TwoDaysReport", "Attendances")><i class="fa fa-user"></i> <span>Last MWF/TTS Report</span></a></li>
                            <li><a href=@Url.Action("UnMarkBatches", "Attendances")><i class="fa fa-user"></i> <span>Unmark Batches Report</span></a></li>
                            <li><a href=@Url.Action("VoucherReport", "Vouchers")><i class="fa fa-list"></i> <span>Voucher Report</span></a></li>
                            <li><a href=@Url.Action("PasswordChange", "Management")><i class="fa fa-dashboard"></i> <span>Change Password</span></a></li>
                        </ul>

                        #endregion
                    }
                    else if (Session["UserRole"].ToString() == "3")
                    {
                        #region Admission Side MENUs
                        <ul class="sidebar-menu" data-widget="tree">
                            <li><a href=@Url.Action("Index", "Admission")><i class="fa fa-dashboard"></i> <span>Dashboard</span></a></li>
                            <li class="treeview">
                                <a href="@Url.Action("Index", "Batches")">
                                    <i class="fa fa-link"></i> <span>Batches</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href=@Url.Action("Index", "Batches", new { Status = "In Progress" })><i class="fa fa-hourglass-start"></i> <span>Batches in Progress</span></a></li>
                                    <li><a href=@Url.Action("Index", "Batches", new { Status = "Not Yet Started" })><i class="fa fa-hourglass-half"></i> <span>Upcoming Batches</span></a></li>
                                    <li><a href=@Url.Action("Index", "Batches", new { Status = "Course Completed" })><i class="fa fa-hourglass-end"></i> <span>Completed Batches</span></a></li>
                                </ul>
                            </li>
                            <!--Students List-->
                            <li class="treeview">
                                <a href="@Url.Action("Index", "Students")">
                                    <i class="fa fa-user"></i> <span>Students</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href=@Url.Action("Index", "Students", new { Status = "Enrolled" })><i class="fa fa-address-card"></i> <span>Enrolled Students</span></a></li>
                                    <li><a href=@Url.Action("Index", "Students", new { Status = "Drop out" })><i class="fa fa-address-book-o"></i> <span>Drop Out Students</span></a></li>
                                    <li><a href=@Url.Action("Index", "Students", new { Status = "Course Completed" })><i class="fa fa-address-card-o"></i> <span>Course Completed Students</span></a></li>
                                </ul>
                            </li>

                            <li><a href=@Url.Action("Index", "StudentBatches")><i class="fa fa-dashboard"></i> <span>Student Batch</span></a></li>
                        </ul>

                        #endregion
                    }
                    else if (Session["UserRole"].ToString() == "4")
                    {
                        #region Coordinators Side MENUs
                        <ul class="sidebar-menu" data-widget="tree">
                            <li><a href=@Url.Action("Index", "Coordinator")><i class="fa fa-dashboard"></i> <span>Dashboard</span></a></li>
                            <li><a href=@Url.Action("Index", "Books")><i class="fa fa-dashboard"></i> <span>Book</span></a></li>
                            <li><a href=@Url.Action("Index", "Batches")><i class="fa fa-dashboard"></i> <span>Batch</span></a></li>
                            <li><a href=@Url.Action("Index", "Students")><i class="fa fa-dashboard"></i> <span>Students</span></a></li>
                            <li><a href=@Url.Action("Index", "BatchBooks")><i class="fa fa-dashboard"></i> <span>Batch Book</span></a></li>
                            <li><a href=@Url.Action("Index", "BatchTeacherDetails")><i class="fa fa-dashboard"></i> <span>Batch Teacher</span></a></li>
                            <li><a href=@Url.Action("Index", "StudentBatches")><i class="fa fa-dashboard"></i> <span>Student Batch</span></a></li>
                        </ul>

                        #endregion
                    }


                }



                <!--/.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                @RenderSection("contentHeader", false)
            </section>
            <!-- Main content -->
            <section class="content container-fluid">
                <section class="content">
                    <div class="loader"></div>
                    @RenderBody()
                </section>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">
                Aptech Computer Education - <small>Shahrah-e-Faisal</small>
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2018</strong> All rights reserved.
        </footer>
    </div>
</body>
</html>